{
  "name": "react-cordova-file-manager",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "serve": "^12.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build/ www/",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "serve": "serve -s www",
    "build-and-serve": "npm run build && npm run serve"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cordova-android": "^9.1.0",
    "cordova-browser": "^6.0.0"
  },
  "cordova": {
    "platforms": [
      "android",
      "browser"
    ]
  }
}